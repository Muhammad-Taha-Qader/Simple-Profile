{
  "builds": [
    {
      "src": "src/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "src/output.css",
      "use": "@vercel/static"
    },
    {
      "src": "img/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/src/index.html"
    },
    {
      "src": "/output.css",
      "dest": "/src/output.css"
    },
    {
      "src": "/img/(.*)",
      "dest": "/img/$1"
    }
  ]
}
